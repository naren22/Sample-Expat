                            <logoImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Enter logo Image Path. Mandatory"
                                fieldLabel="Logo"
                                name="./logoImage"/>
                            <logoLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Target link for logo . Must start with http:// or https://."
                                fieldLabel="Provide the link for logo "
                                name="./logoLink"
                                rootPath="/content/expatportal"/>
                            <logoAltText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
