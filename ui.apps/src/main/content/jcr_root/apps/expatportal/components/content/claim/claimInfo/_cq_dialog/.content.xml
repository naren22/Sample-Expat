<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Claim Info"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Service Endpoint"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tokenServiceEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter token service endpoint"
                                fieldLabel="token service endpoint"
                                maxLength="100"
                                name="./tokenServiceEndpoint"/>
                            <memberServiceEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter member service endpoint"
                                fieldLabel="member service endpoint"
                                maxLength="100"
                                name="./memberServiceEndpoint"/>
                            <memberServiceURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter member service url"
                                fieldLabel="member service url"
                                maxLength="100"
                                name="./memberServiceURL"/>
                            <providerServiceEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter provider service endpoint"
                                fieldLabel="provider service endpoint"
                                maxLength="100"
                                name="./providerServiceEndpoint"/>
                            <providerServiceURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="provider service url"
                                fieldLabel="provider url"
                                maxLength="100"
                                name="./providerServiceURL"/>
                            <countryServiceEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter country service endpoint"
                                fieldLabel="country service endpoint"
                                maxLength="100"
                                name="./countryServiceEndpoint"/>
                            <countryServiceURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter country service url"
                                fieldLabel="country service url"
                                maxLength="100"
                                name="./countryServiceURL"/>
                            <currencyServiceEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter currency service endpoint"
                                fieldLabel="currency service endpoint"
                                maxLength="100"
                                name="./currencyServiceEndpoint"/>
                            <currencyServiceURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter currency service url"
                                fieldLabel="currency service url"
                                maxLength="100"
                                name="./currencyServiceURL"/>
                            <paymentPreferencesEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter payment preferences endpoint"
                                fieldLabel="payment preferences url"
                                maxLength="100"
                                name="./paymentPreferencesEndpoint"/>
                            <paymentPreferencesServiceURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter payment preferences servlet url"
                                fieldLabel="payment preferences servlet url"
                                maxLength="100"
                                name="./paymentPreferencesServiceURL"/>
                            <onetimepaymentEndpoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter onetimepayment service endpoint"
                                fieldLabel="onetimepayment service endpoint"
                                maxLength="100"
                                name="./onetimepaymentEndpoint"/>
                            <onetimepaymentServiceURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter onetimepayment service url"
                                fieldLabel="onetimepayment service url"
                                maxLength="100"
                                name="./onetimepaymentServiceURL"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Tab 2"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <memberFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Member field"
                                fieldLabel="member field"
                                maxLength="100"
                                name="./memberFieldText"/>
                            <providerFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Provider field"
                                fieldLabel="provider firld"
                                maxLength="100"
                                name="./providerFieldText"/>
                            <providercountryFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Provider country field"
                                fieldLabel="provider country field"
                                maxLength="100"
                                name="./providercountryFieldText"/>
                            <providercityFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Provider city field"
                                fieldLabel="Provider city field"
                                maxLength="100"
                                name="./providercityFieldText"/>
                            <claimnicknameFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Claim nick name field"
                                fieldLabel="claim nick name field"
                                maxLength="100"
                                name="./claimnicknameFieldText"/>
                            <billedamountFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Billed amount field"
                                fieldLabel="billed amount field"
                                maxLength="100"
                                name="./billedamountFieldText"/>
                            <claimcurrencyFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Claim currency field"
                                fieldLabel="claim currency field"
                                maxLength="100"
                                name="./claimcurrencyFieldText"/>
                            <dateofserviceFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Dateofservice field"
                                fieldLabel="Date of Service field"
                                maxLength="100"
                                name="./dateofserviceFieldText"/>
                            <diagnosisFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Diagnosis field"
                                fieldLabel="diagnosis field"
                                maxLength="100"
                                name="./diagnosisFieldText"/>
                            <diagnosistext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Diagnosis text"
                                fieldLabel="diagnosis text"
                                maxLength="100"
                                name="./diagnosistext"/>
                            <descriptionoftreatmentFieldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Descriptionoftreatment field"
                                fieldLabel="description of treatment field"
                                maxLength="100"
                                name="./descriptionoftreatmentFieldText"/>
                            <descriptionoftreatmenttext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Description of treatment text"
                                fieldLabel="description of treatment text"
                                maxLength="100"
                                name="./descriptionoftreatmenttext"/>
                            <cancelButtonUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Cancel button URL"
                                fieldLabel="cancel button url"
                                maxLength="100"
                                name="./cancelButtonUrl"/>
                        </items>
                    </columns>
                </items>
            </tab2>
            <notes
                jcr:primaryType="nt:unstructured"
                jcr:title="Notes"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <infoTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter info title"
                                fieldLabel="Enter info title"
                                key="infoTitle"
                                maxlength="50"
                                name="./infoTitle"/>
                            <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                name="./infoText"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <findreplace
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <subsuperscript
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <spellcheck
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                    <misctools
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                            <chars jcr:primaryType="nt:unstructured">
                                                <copyright
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#169;"/>
                                                <trademark
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#8482;"/>
                                                <registered
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#174;"/>
                                                <emDash
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#8212;"/>
                                                <pound
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#163;"/>
                                                <nbsp
                                                    jcr:primaryType="nt:unstructured"
                                                    entity="&amp;#160;"/>
                                            </chars>
                                        </specialCharsConfig>
                                    </misctools>
                                    <styles
                                        jcr:primaryType="nt:unstructured"
                                        features="*">
                                        <styles jcr:primaryType="nt:unstructured">
                                            <plainSpan
                                                jcr:primaryType="nt:unstructured"
                                                cssName="defaultSpan"
                                                text="Default (add span tag)"/>
                                            <lead
                                                jcr:primaryType="nt:unstructured"
                                                cssName="lead"
                                                text="Lead"/>
                                        </styles>
                                    </styles>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                    ref="justify"/>
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists"/>
                                                <paraformat
                                                    jcr:primaryType="nt:unstructured"
                                                    items="paraformat:getFormats:paraformat-pulldown"
                                                    ref="paraformat"/>
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                            </text>
                        </items>
                    </columns>
                </items>
            </notes>
        </items>
    </content>
</jcr:root>
