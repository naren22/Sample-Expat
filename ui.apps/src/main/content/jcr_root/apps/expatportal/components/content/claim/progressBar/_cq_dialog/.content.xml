<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Progress Bar"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Main"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter section title"
                                fieldLabel="section title"
                                maxLength="100"
                                name="./sectionTitle"/>
                            <claimInfoText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter claim info text"
                                fieldLabel="claim info"
                                maxLength="100"
                                name="./claimInfoText"/>
                            <uploadFileText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter upload file text"
                                fieldLabel="upload text"
                                maxLength="100"
                                name="./uploadFileText"/>
                            <reviewText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                allowBlank="{Boolean}false"
                                fieldDescription="Enter review title"
                                fieldLabel="Review Text"
                                maxLength="100"
                                name="./reviewText"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <steps
                jcr:primaryType="nt:unstructured"
                jcr:title="Steps-Detail"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <steps
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                defaultValue="claimUpload"
                                fieldDescription="Class for Step, Use  comma to seprate the classname"
                                fieldLabel="Class for Step, Use  comma to seprate the classname"
                                key="infoTitle"
                                maxlength="50"
                                name="./steps"
                                value="claimUpload"/>
                        </items>
                    </columns>
                </items>
            </steps>
        </items>
    </content>
</jcr:root>
