<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <mainheading
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Main Heading"
                fieldLabel="main heading"
                maxLength="100"
                name="./mainheading"/>
            <subheading
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Sub Heading"
                fieldLabel="sub heading"
                maxLength="100"
                name="./subheading"/>
            <patientname
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Patient Name"
                fieldLabel="patient name"
                maxLength="100"
                name="./patientname"/>
            <diagnosis_symptoms
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Diagnosis Symptoms"
                fieldLabel="diagnosis symptoms"
                maxLength="100"
                name="./diagnosis_symptoms"/>
            <diagnosis_symptomsText
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Diagnosis Symptoms Text"
                fieldLabel="diagnosis symptoms text"
                maxLength="100"
                name="./diagnosis_symptomsText"/>
            <servicesprocedure
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Services Procedure"
                fieldLabel="services procedure"
                maxLength="100"
                name="./servicesprocedure"/>
            <dateofservice
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Date of Service "
                fieldLabel="Date of Service"
                maxLength="100"
                name="./dateofservice"/>
            <dateofservicetext
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="dateofservicetext"
                fieldLabel="dateofservicetext"
                maxLength="100"
                name="./dateofservicetext"/>
            <chooseprovider_bar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Choose Provider Bar"
                fieldLabel="choose provider bar"
                maxLength="100"
                name="./chooseprovider_bar"/>
            <filloutguaranteeofpayment_bar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="fill out guarantee of payment bar"
                fieldLabel="fill out guarantee of payment bar"
                maxLength="100"
                name="./filloutguaranteeofpayment_bar"/>
            <reviewsubmitrequest_bar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="review submit request bar"
                fieldLabel="review submit request bar"
                maxLength="100"
                name="./reviewsubmitrequest_bar"/>
            <step3mainheading
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Step 3 Main Heading"
                fieldLabel="step 3 main heading"
                maxLength="100"
                name="./step3mainheading"/>
            <step3subheading
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/form/textfield"
                allowBlank="{Boolean}false"
                fieldDescription="Step 3 Sub Heading"
                fieldLabel="step 3 sub heading"
                maxLength="100"
                name="./step3subheading"/>
        </items>
    </content>
</jcr:root>
